{
  "manifest_version": 2,
  "name": "YouTube Video Downloader",
  "version": "0.1.0",
  "description": "This extension allows you to download YouTube videos",
  "browser_action": {
    "default_popup": "index.html"
  },
  "content_scripts":[
    {
      "matches":["https://*.youtube.com/*"],
      "js":["index.js"]
    }
  ],
  "background":{
    "scripts" :["backgroung.js"]
  }
}
